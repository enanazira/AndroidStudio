<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/background3" />

    <View
        android:id="@+id/divider"
        android:layout_width="678dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="#9A9163E7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextView
        android:id="@+id/lbl_biodata"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Biodata"
        android:textColor="@android:color/white"
        android:textSize="20sp"
        app:autoSizeTextType="uniform"
        app:fontFamily="@font/roboto_slab"
        app:layout_constraintBottom_toBottomOf="@+id/divider"
        app:layout_constraintEnd_toEndOf="@+id/divider"
        app:layout_constraintStart_toStartOf="@+id/divider"
        app:layout_constraintTop_toTopOf="@+id/divider" />

    <ImageView
        android:id="@+id/img_profil"
        android:layout_width="107dp"
        android:layout_height="99dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toStartOf="@+id/tv_name"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_person" />

    <TextView
        android:id="@+id/tv_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kolej Ungku Omar"
        android:textColor="@android:color/black"
        android:textSize="30sp"
        app:fontFamily="@font/roboto_slab"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.609"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <TextView
        android:id="@+id/tv_matric"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="A123456"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        app:fontFamily="@font/roboto_slab"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/tv_name"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="@+id/tv_name"
        app:layout_constraintTop_toBottomOf="@+id/tv_name"
        app:layout_constraintVertical_bias="1.0" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider"
        app:layout_constraintVertical_bias="1.0">

        <View
            android:id="@+id/divider4"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="?android:attr/listDivider"
            app:layout_constraintEnd_toStartOf="@+id/tv_sJantina"
            app:layout_constraintHorizontal_bias="0.533"
            app:layout_constraintStart_toEndOf="@+id/tv_jantina"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_blok"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Blok"
            android:textSize="14sp"
            app:fontFamily="@font/roboto_medium"
            app:layout_constraintBottom_toTopOf="@+id/tv_fakulti"
            app:layout_constraintStart_toStartOf="@+id/tv_fakulti"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_fakulti"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Fakulti"
            android:textSize="14sp"
            app:fontFamily="@font/roboto_medium"
            app:layout_constraintBottom_toTopOf="@+id/tv_jantina"
            app:layout_constraintStart_toStartOf="@+id/tv_jantina"
            app:layout_constraintTop_toBottomOf="@+id/tv_blok" />

        <TextView
            android:id="@+id/tv_jantina"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Jantina"
            android:textSize="14sp"
            app:fontFamily="@font/roboto_medium"
            app:layout_constraintBottom_toTopOf="@+id/tv_agama"
            app:layout_constraintStart_toStartOf="@+id/tv_agama"
            app:layout_constraintTop_toBottomOf="@+id/tv_fakulti" />

        <TextView
            android:id="@+id/tv_kelab"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Kelab/Biro"
            android:textSize="14sp"
            app:fontFamily="@font/roboto_medium"
            app:layout_constraintBottom_toTopOf="@+id/tv_jawatan"
            app:layout_constraintStart_toStartOf="@+id/tv_jawatan"
            app:layout_constraintTop_toBottomOf="@+id/tv_agama" />

        <TextView
            android:id="@+id/tv_jawatan"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="139dp"
            android:layout_marginLeft="139dp"
            android:text="Jawatan"
            android:textSize="14sp"
            app:fontFamily="@font/roboto_medium"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_kelab" />

        <TextView
            android:id="@+id/tv_agama"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Agama"
            android:textSize="14sp"
            app:fontFamily="@font/roboto_medium"
            app:layout_constraintBottom_toTopOf="@+id/tv_kelab"
            app:layout_constraintStart_toStartOf="@+id/tv_kelab"
            app:layout_constraintTop_toBottomOf="@+id/tv_jantina" />

        <TextView
            android:id="@+id/tv_sFakulti"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="- Fakulti Teknologi Dan Sains Maklumat"
            android:textSize="14sp"
            app:fontFamily="@font/roboto_medium"
            app:layout_constraintBottom_toTopOf="@+id/tv_sJantina"
            app:layout_constraintStart_toStartOf="@+id/tv_sJantina"
            app:layout_constraintTop_toBottomOf="@+id/tv_sBlok" />

        <TextView
            android:id="@+id/tv_sBlok"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="- Bilik"
            android:textSize="14sp"
            app:fontFamily="@font/roboto_medium"
            app:layout_constraintBottom_toTopOf="@+id/tv_sFakulti"
            app:layout_constraintEnd_toEndOf="@+id/tv_sFakulti"
            app:layout_constraintStart_toStartOf="@+id/tv_sFakulti"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_sJantina"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="- Lelaki"
            android:textSize="14sp"
            app:fontFamily="@font/roboto_medium"
            app:layout_constraintBottom_toTopOf="@+id/tv_sAgama"
            app:layout_constraintStart_toStartOf="@+id/tv_sAgama"
            app:layout_constraintTop_toBottomOf="@+id/tv_sFakulti" />

        <TextView
            android:id="@+id/tv_sAgama"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="- Buddha"
            android:textSize="14sp"
            app:fontFamily="@font/roboto_medium"
            app:layout_constraintBottom_toTopOf="@+id/tv_sKelab"
            app:layout_constraintStart_toStartOf="@+id/tv_sKelab"
            app:layout_constraintTop_toBottomOf="@+id/tv_sJantina" />

        <TextView
            android:id="@+id/tv_sKelab"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="-Kelab Ramah Mesra"
            android:textSize="14sp"
            app:fontFamily="@font/roboto_medium"
            app:layout_constraintBottom_toTopOf="@+id/tv_sJawatan"
            app:layout_constraintStart_toStartOf="@+id/tv_sJawatan"
            app:layout_constraintTop_toBottomOf="@+id/tv_sAgama" />

        <TextView
            android:id="@+id/tv_sJawatan"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="281dp"
            android:layout_marginRight="281dp"
            android:text="- Presiden"
            android:textSize="14sp"
            app:fontFamily="@font/roboto_medium"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_sKelab" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>